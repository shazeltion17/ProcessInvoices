  <Sequence sap2010:Annotation.AnnotationText="Close any running SAP instances." DisplayName="Closing SAP" sap2010:WorkflowViewState.IdRef="Sequence_12">
    <Sequence.Variables>
      <Variable x:TypeArguments="x:Int32" Default="0" Name="RetryCount" />
      <Variable x:TypeArguments="x:Boolean" Name="SAPWindowOpened" />
      <Variable x:TypeArguments="x:Boolean" Name="SAPLogonOpened" />
    </Sequence.Variables>
    <DoWhile sap2010:Annotation.AnnotationText="Try 3 times to close SAP window." DisplayName="Do while - Close SAP Window" sap2010:WorkflowViewState.IdRef="DoWhile_1" Condition="[SAPWindowOpened OrElse SAPLogonOpened]">
      <Sequence DisplayName="Close any open SAP window" sap2010:WorkflowViewState.IdRef="Sequence_10">
        <If Condition="[RetryCount &gt; 3]" DisplayName="If - retryCount &gt; 3" sap2010:WorkflowViewState.IdRef="If_6">
          <If.Then>
            <Throw DisplayName="Throw - Failed to close SAP" Exception="[new ApplicationException(&quot;Failed to close SAP Window&quot;)]" sap2010:WorkflowViewState.IdRef="Throw_3" />
          </If.Then>
        </If>
        <ui:UiElementExists DisplayName="Element Exists - SAP Window" Exists="[SAPWindowOpened]" sap2010:WorkflowViewState.IdRef="UiElementExists_4">
          <ui:UiElementExists.Target>
            <ui:Target ClippingRegion="{x:Null}" Element="{x:Null}" Selector="&lt;wnd app='saplogon.exe' cls='SAP_FRONTEND_SESSION' title='*' /&gt;" TimeoutMS="3000" WaitForReady="INTERACTIVE" />
          </ui:UiElementExists.Target>
        </ui:UiElementExists>
        <ui:UiElementExists DisplayName="Element Exists - SAP Logon Window" Exists="[SAPLogonOpened]" sap2010:WorkflowViewState.IdRef="UiElementExists_5">
          <ui:UiElementExists.Target>
            <ui:Target ClippingRegion="{x:Null}" Element="{x:Null}" Selector="&lt;wnd app='saplogon.exe' cls='#32770' title='SAP Logon*' /&gt;" TimeoutMS="3000" WaitForReady="INTERACTIVE" />
          </ui:UiElementExists.Target>
        </ui:UiElementExists>
        <If Condition="[SAPWindowOpened]" DisplayName="If - sapWindowOpened" sap2010:WorkflowViewState.IdRef="If_5">
          <If.Then>
            <Sequence DisplayName="Close application and retryCount++" sap2010:WorkflowViewState.IdRef="Sequence_11">
              <ui:CloseApplication DisplayName="Close application 'saplogon.exe SAP_FRONTEND_SESSION'" sap2010:WorkflowViewState.IdRef="CloseApplication_1">
                <ui:CloseApplication.Target>
                  <ui:Target ClippingRegion="{x:Null}" Element="{x:Null}" Selector="&lt;wnd app='saplogon.exe' cls='SAP_FRONTEND_SESSION' title='*' /&gt;" TimeoutMS="3000" WaitForReady="INTERACTIVE" />
                </ui:CloseApplication.Target>
              </ui:CloseApplication>
              <Assign DisplayName="Assign - retryCount++" sap2010:WorkflowViewState.IdRef="Assign_4">
                <Assign.To>
                  <OutArgument x:TypeArguments="x:Int32">[RetryCount]</OutArgument>
                </Assign.To>
                <Assign.Value>
                  <InArgument x:TypeArguments="x:Int32">[RetryCount + 1]</InArgument>
                </Assign.Value>
              </Assign>
            </Sequence>
          </If.Then>
          <If.Else>
            <If Condition="[SAPLogonOpened]" DisplayName="If - sapLogonOpened" sap2010:WorkflowViewState.IdRef="If_7">
              <If.Then>
                <Sequence DisplayName="Close logon window and retryCount++" sap2010:WorkflowViewState.IdRef="Sequence_13">
                  <ui:CloseApplication DisplayName="Close application 'saplogon.exe SAP Logon*'" sap2010:WorkflowViewState.IdRef="CloseApplication_2">
                    <ui:CloseApplication.Target>
                      <ui:Target ClippingRegion="{x:Null}" Element="{x:Null}" Selector="&lt;wnd app='saplogon.exe' cls='#32770' title='SAP Logon*' /&gt;" TimeoutMS="3000" WaitForReady="INTERACTIVE" />
                    </ui:CloseApplication.Target>
                  </ui:CloseApplication>
                  <Assign DisplayName="Assign - retryCount++" sap2010:WorkflowViewState.IdRef="Assign_3">
                    <Assign.To>
                      <OutArgument x:TypeArguments="x:Int32">[RetryCount]</OutArgument>
                    </Assign.To>
                    <Assign.Value>
                      <InArgument x:TypeArguments="x:Int32">[RetryCount + 1]</InArgument>
                    </Assign.Value>
                  </Assign>
                </Sequence>
              </If.Then>
            </If>
          </If.Else>
        </If>
      </Sequence>
    </DoWhile>
